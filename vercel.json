{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/router/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}
